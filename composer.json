{
    "name": "johnsmithamk/message", 
    "description": "Sample Package in Vendor folder for laravel",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "John Smith",
            "email": "testing7672@gmail.com"
        }
    ],  

    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/johnsmithamk/message.git"
        }
    ],

    "require": {
       "johnsmithamk/message": "dev-master",
       "php": ">=7.0.0",
    }, 

    "require-dev": {
        "illuminate/database": "5.5.*",
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~5.4"
    },

    "autoload":{
        "psr-4":{
            "test\\message\\": "src"
        }
    },

    "extra": {
             
        "laravel": {
            "providers": [
                "test\\message\\MessageServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true

}
 